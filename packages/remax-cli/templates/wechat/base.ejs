<template name="REMAX_TPL">
  <block wx:for="{{tree.root.children}}" wx:key="{{item.id}}">
    <template is="{{'REMAX_TPL_ELEMENT_' + item.props.__id__}}" data="{{i: item}}" />
  </block>
</template>

<% for (let component of components) { %>
  <%- include('./element.ejs', {
    type: component.type,
    props: component.props,
    id: component.id,
  }) %>
<% } %>